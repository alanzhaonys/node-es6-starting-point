{
  "name": "node-es6-starting-point",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec --require babel-core/register test/*",
    "cover": "./node_modules/.bin/nyc --extension .js --reporter=lcov --reporter=text ./node_modules/.bin/mocha --require babel-core/register ./test/*",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./yarn.lock --copy-files",
    "start": "yarn run build && node dist/index.js"
  },
  "author": "Alan Zhao",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3"
  }
}

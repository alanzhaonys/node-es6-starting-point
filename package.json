{
  "name": "node-es6-starting-point",
  "version": "1.0.4",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./lib/* && eslint ./test/*",
    "test": "mocha --reporter spec --require babel-core/register ./test/*",
    "cover": "nyc --extension .js --reporter=lcov --reporter=text mocha --require babel-core/register ./test/*",
    "doc": "jsdoc ./lib/* -d ./doc",
    "build": "rimraf ./dist && eslint ./lib/* && babel ./lib --out-dir ./dist --copy-files",
    "clify": "cp-cli ./dist/index.js ./dist/cli.js && append-top ./dist/cli.js '#!/usr/bin/env node' && make-exec ./dist/cli.js",
    "start": "yarn build && node ./dist/index.js",
    "package": "yarn build && yarn clify && pkg ."
  },
  "bin": {
    "node-es6-starting-point": "./dist/cli.js"
  },
  "author": "Alan Zhao",
  "license": "MIT",
  "devDependencies": {
    "append-top": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "cp-cli": "^2.0.0",
    "eslint": "^6.0.1",
    "jsdoc": "^3.6.3",
    "make-exec": "^1.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "pkg": "^4.4.0",
    "rimraf": "^2.6.3"
  }
}
